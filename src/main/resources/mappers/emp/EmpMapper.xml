<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dq.aquaranth.emp.mapper.EmpMapper">

    <select id="empList" resultType="com.dq.aquaranth.emp.dto.EmpDTO">
        select * from emp where emp_no > 0
    </select>

    <select id="empSelectOne" resultType="com.dq.aquaranth.emp.dto.EmpDTO">
        select * from emp where emp_no = #{emp_no}
    </select>

</mapper>
