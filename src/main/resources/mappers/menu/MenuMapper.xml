<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dq.aqaranth.domain.menu.mapper.MenuMapper">

    <select id="findAllMenus" resultType="com.dq.aqaranth.domain.menu.dto.response.MenuResponse">
        SELECT * FROM menu
    </select>

    <select id="findByGnBMenus" resultType="com.dq.aqaranth.domain.menu.dto.response.MenuResponse">
        SELECT * FROM menu WHERE upper_menu_no IS NULL
    </select>


</mapper>
